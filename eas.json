{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BASE": "http://localhost:8080",
        "EXPO_PUBLIC_ANDROID_BASE": "http://10.0.2.2:8080",
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_URL": "http://172.201.224.249"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_BASE": "http://localhost:8080",
          "EXPO_PUBLIC_ANDROID_BASE": "http://10.0.2.2:8080",
          "EXPO_PUBLIC_ENV": "production",
          "EXPO_PUBLIC_URL": "http://172.201.224.249"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BASE": "http://localhost:8080",
        "EXPO_PUBLIC_ANDROID_BASE": "http://10.0.2.2:8080",
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_URL": "http://172.201.224.249"
      }
    }
  },
  "submit": {
    "production": {}
  }
